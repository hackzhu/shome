{
  "code-runner.executorMap": {
    "python": "/usr/bin/env python"
  },
  "files.associations": {
    "*.yaml": "home-assistant"
},
"code-runner.runInTerminal": false,
"MicroPython.executeButton": [
    {
        "text": "▶",
        "tooltip": "运行",
        "alignment": "left",
        "command": "extension.executeFile",
        "priority": 3.5
    }
],
"MicroPython.syncButton": [
    {
        "text": "$(sync)",
        "tooltip": "同步",
        "alignment": "left",
        "command": "extension.execute",
        "priority": 4
    }
]
}
